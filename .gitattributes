# Set default behaviour to automatically normalize line endings.
* text=auto
# Force BASH scripts to always use Unix-style line endings.
*.sh text eol=lf
app/main.py filter=lfs diff=lfs merge=lfs -text
app/static filter=lfs diff=lfs merge=lfs -text
app/templates filter=lfs diff=lfs merge=lfs -text
app/tests filter=lfs diff=lfs merge=lfs -text
app/__init__.py filter=lfs diff=lfs merge=lfs -text
app/aws.py filter=lfs diff=lfs merge=lfs -text
app/spotify.py filter=lfs diff=lfs merge=lfs -text
app/viz.py filter=lfs diff=lfs merge=lfs -text
app/db.py filter=lfs diff=lfs merge=lfs -text
app/ml.py filter=lfs diff=lfs merge=lfs -text
